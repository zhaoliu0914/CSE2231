PROGRAM Test IS

  INSTRUCTION one IS
     move
     turnleft
  END name-does-not-match
  
  INSTRUCTION two IS
    one
    IF next-is-not-empty THEN
      turnleft
    ELSE
      one
      one
    END IF
  END two

BEGIN
  infect
  WHILE true DO
    two
    two
    one
  END WHILE
  IF next-is-empty THEN
    move
  END IF
  IF next-is-not-empty THEN
    turnleft
  ELSE
    infect
    one
  END IF
END Test
